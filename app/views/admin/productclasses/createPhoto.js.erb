if("<%= @photo.id.to_s.html_safe %>"){
	var image = new Object();
	image.type = "img";
	image.id = "<%= @photo.id.to_s.html_safe %>";
	image.path = "<%= @photo.image.to_s.html_safe %>";
	
	if($("#newImageLink").val()){
		image.link = $("#newImageLink").val();
	}

	editor.resetChild();
	editor.img.update(image);
	
}
else{
	<% @errors = "" %>
	<%if @photo.errors %>
	<% @photo.errors.full_messages.each do |msg| %>
	<% @errors +=  msg %>
	<% end %>
	<% end %>

	if("<%= @errors %>"){
		editor.alert("<%= @errors %>", "error");
	}
	
}
